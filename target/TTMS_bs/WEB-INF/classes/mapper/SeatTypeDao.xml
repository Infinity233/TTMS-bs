<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.Infinity.dao.SeatTypeDao">

    <select id="selectAll" resultType="seatType">
        select * from t_seatType
    </select>

    <select id="selectById" resultType="seatType" parameterType="integer">
        select * from t_seatType where id = #{id}
    </select>

</mapper>


